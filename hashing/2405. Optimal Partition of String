2405. Optimal Partition of String
https://leetcode.com/problems/optimal-partition-of-string/solutions/7254443/hashing-by-shubham_sk70-dtzf/


class Solution {
public:
    int partitionString(string s) {
        int n = s.length();
        int cnt = 0;
        int st = 0;

        vector<int> l_s(26,-1);

        for(int i = 0; i < n; i++){
            char ch = s[i];
            if(l_s[ch - 'a'] >= st){
                cnt++;
                st = i;
            }
            l_s[ch - 'a']=i;
        }
        return cnt+1;
    }
};
