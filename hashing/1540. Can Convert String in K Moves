1540. Can Convert String in K Moves

https://leetcode.com/problems/can-convert-string-in-k-moves/solutions/7246043/hashing/

class Solution {
public:
    bool canConvertString(string s, string t, int k) {
        unordered_map<int,int> count;
        int n=s.size();
        int sum=0;
        if(s.size()!=t.size())
        {
            return false;
        }
        for(int i=0;i<n;i++)
        {
            long long diff=(t[i]-s[i]+26)%26;
            if(diff==0)
            {
                continue;
            }
            int move = diff+26*count[diff];
            if(move>k)
            {
                return false;
            }
            count[diff]++;
        }
        return true;
    }
};
